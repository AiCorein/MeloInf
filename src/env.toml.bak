[bot]
# bot 的项目名称（若无再次开发，可不修改）
bot_proj_name = "meloinf"
# bot 的版本（若无再次开发，可不修改）
bot_proj_ver = "1.0.0"
# bot 的项目地址（若无再次开发，可不修改）
bot_proj_src = "https://github.com/aicorein/meloinf"

# bot 的正式名称
bot_name = "meloinf-stable"
# bot 的昵称
bot_nickname = ["melo", "cute melo", "cute-melo"]

# 统一的命令起始符
uni_cmd_start = ["."]
# 统一的命令分隔符
uni_cmd_sep = [' ']

# 若使用 http 代理，对应的地址
http_proxy = 'http://127.0.0.1:7890'

# 使用文本转图片功能时，使用的字体
txt2img_font = 'data/SarasaMonoSC-Regular.ttf'
# 使用 matplotlib 绘图时，使用的字体
figure_font = 'data/SarasaMonoSC-Regular.ttf'

# 自动匹配回复的词库文件路径
word_lib = "data/wordlib/words.txt"

# dice 模拟器的配置文件路径
dice_config = "data/dice/config.toml"
# dice 模拟器的牌堆文件夹路径
dice_data_dir = "data/dice/data"
# dice 模拟器的基础骰子设置
dice_base_r = [1, 100]

# 和风天气 API key
weather_key = ""

# 百度翻译 appid
baidu_translate_appid = ""
# 百度翻译 api key
baidu_translate_key = ""
# 百度翻译所用的 hash salt
hash_salt = ""

# 每日新闻推送的时间
everyday_news_time = "08:30:00"

[onebot]
# 前向 ws 服务地址
forward_ws = "ws://127.0.0.1:8080"
# 反向 ws 服务的 host（留空则不使用反向）
reverse_host = ""
# 反向 ws 服务的 port（留空则不使用反向）
reverse_port = ""
# 与 onebot 实现端通信时使用的 access_token（留空则不使用）
access_token = ""

# onebot 协议下的 owner
owner_id = 10001
# owner 的名称
owner_names = ["owner-name1", "owner-name2"]
# 超级用户列表
super_users = []
# 白名单用户列表
white_users = []
# 黑名单用户列表
black_users = [10002, 10003]
# 可触发操作的群聊白名单
white_groups = [10004, 10005]

# 每日新闻推送的群聊
everyday_news_group = [10004]

[http_headers]
# 进行 http 请求时的 User-Agent
User-Agent = 'Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/88.0.4324.96 Safari/537.36'